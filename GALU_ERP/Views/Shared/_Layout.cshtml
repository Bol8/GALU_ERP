                        <span class="icon-bar"></span>
                    </button>
                    @*@Html.ActionLink("AMS", "Index", "Home", null, new { @class = "navbar-brand" })*@
                </div>
                <div class="navbar-collapse collapse">
                    @Html.Partial("_PartialNav")
                </div>

            </div>
        </div>
