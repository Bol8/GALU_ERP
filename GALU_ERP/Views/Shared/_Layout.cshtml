            });

            $("#menuCompras").click(function () {
                $("#ul_Compras").slideToggle();
            });

            $(".img").on({
                mouseover: function () {
                   // $(this).css("color", "red");
                    $(this).css('cursor', 'pointer');
                }

                //mouseleave: function () {
                //    $(this).css("background-color", "lightblue");
                //},
                //click: function () {
                //   alert()
                //}
            });

            $("#l").sidr("toggle", "sidr", "none");


        });

